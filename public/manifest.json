{
  "name": "格式化时间戳",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*:*/*"
  ],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*:*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}